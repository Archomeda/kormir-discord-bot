{
  "name": "kormir-discord-bot",
  "version": "0.1.0",
  "description": "A bot that's powering Legacy of the Six on Discord.",
  "repository": "Archomeda/kormir-discord-bot",
  "author": "Archomeda",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "lint": "xo",
    "test": "echo \"Tests are currently not maintained\"",
    "cover": "echo \"Coverage is currently not maintained\""
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "discord.js": "^11.1.0",
    "feedparser": "^2.2.1",
    "filesize": "^3.5.10",
    "gw2api-client": "^5.6.2",
    "gw2e-chat-codes": "^1.2.0",
    "i18next": "^8.4.3",
    "i18next-node-fs-backend": "^1.0.0",
    "js-yaml": "^3.9.1",
    "lodash": "^4.17.4",
    "minimist": "Archomeda/minimist#extend-strings",
    "moment": "^2.18.1",
    "moment-timezone": "^0.5.13",
    "mongoose": "^4.11.5",
    "mongoose-sequence": "^3.2.0",
    "mwbot": "^1.0.9",
    "node-cache": "^4.1.1",
    "node-schedule": "^1.2.3",
    "object-hash": "^1.1.8",
    "object-path": "^0.11.4",
    "random-js": "^1.0.8",
    "redis": "^2.7.1",
    "request": "^2.81.0",
    "string-argv": "^0.0.2",
    "sugar-date": "^2.0.4",
    "to-markdown": "^3.1.0"
  },
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo": {
    "ignores": [
      "test/**/*"
    ],
    "space": 4,
    "rules": {
      "camelcase": "warn",
      "linebreak-style": "off",
      "no-div-regex": "off",
      "no-else-return": "warn",
      "no-multiple-empty-lines": [
        "error",
        {
          "max": 2
        }
      ],
      "no-negated-condition": "off",
      "no-prototype-builtins": "off",
      "no-unused-vars": "warn",
      "no-use-extend-native/no-use-extend-native": "off",
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "unicorn/filename-case": "off"
    }
  }
}
